    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "make all",
    "dev": "npm-run-all --continue-on-error --parallel dev:*",
    "dev:css": "sass --watch --load-path . ./app/styles/index.scss ./app/root.css",
    "dev:remix": "cross-env NODE_ENV=development netlify dev",
    "eslint": "eslint ${ESLINT_FORMAT_OPTIONS:-} --ignore-path .gitignore ",
    "lint": "npm-run-all --continue-on-error --parallel lint:*",
    "lint:changelog": "commitlint --from origin/main --to HEAD",
    "prelint:es": "mkdirp reports/style",
    "lint:es": "npm run --silent eslint -- .",
    "prelint:types": "mkdirp reports/style",
    "lint:types": "bash -c \"tsc $TSC_OPTIONS\"",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production netlify dev",
    "prestorybook": "make app/root.css",
    "storybook": "start-storybook -p 6006",
    "test": "TZ=UTC jest",
    "build-storybook": "make storybook-static"
  },
  "devDependencies": {
    "@actions/core": "^1.8.2",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
