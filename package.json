    "wait-on": "^6.0.1",
    "webpack": "^5.72.1"
  },
  "private": true,
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "*.tsx": "npm run eslint -- "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "14",
    "npm": ">=8"
  },
  "sideEffects": false
}
